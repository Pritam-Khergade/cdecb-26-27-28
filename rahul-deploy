apiVersion: apps/v1
kind: Deployment
metadata:
  namespace: rahul
  creationTimestamp: null
  labels:
    app: rahul-deploy
  name: rahul-deploy
spec:
  replicas: 4
  selector:
    matchLabels:
      app: rahul-deploy
  strategy: {}
  template:
    metadata:
      creationTimestamp: null
      labels:
        app: rahul-deploy
    spec:
      containers:
      - image: nginx
        name: nginx
        ports:
        - containerPort: 80
        resources:
          limits:
            cpu: "550m"
            memory: "128Mi"
          requests:
            cpu: "25m"
            memory: "68Mi"
status: {}
